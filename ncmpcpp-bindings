# "albumart clear" takes care of removing albumart when needed
# make the script executable as albumart somewhere on $PATH
#
def_key "f1"
  show_help
  run_external_command "albumart clear"
def_key "1"
  show_playlist
  run_external_command "albumart"
def_key "2"
  show_browser
  run_external_command "albumart clear"
def_key "3"
  show_search_engine
  run_external_command "albumart clear"
def_key "4"
  show_media_library
  run_external_command "albumart clear"
def_key "5"
  show_playlist_editor
  run_external_command "albumart clear"
def_key "6"
  show_tag_editor
  run_external_command "albumart clear"
def_key "7"
  show_outputs
  run_external_command "albumart clear"
def_key "8"
  show_visualizer
  run_external_command "albumart clear"
def_key "="
  show_clock
  run_external_command "albumart clear"
def_key "@"
  show_server_info
  run_external_command "albumart clear"
def_key "I"
  show_artist_info
  run_external_command "albumart clear"

